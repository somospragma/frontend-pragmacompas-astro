---
import RequestsPage from "@/components/page/RequestPages/RequestsPage";
import DashboardLayout from "@/components/templates/DashboardLayout.astro";
import { getSession } from "auth-astro/server";
import type { SessionUser } from "auth.config";

const session = (await getSession(Astro.request)) as SessionUser;
---

<DashboardLayout>
  <RequestsPage client:load session={session} />
</DashboardLayout>
